Missing Number
------------------------
Time Complexity - O(n)
Space Complexity - O(1)
------------------------
class Solution {
    public int missingNumber(int[] nums) {
        int sum= 0;
        int n = nums.length;
// Storing of XOR of all numbers in range
        for(int i=0;i<=n;i++){
            sum ^= i;
        }
// XOR with numbers in array gives number missing from array
        for(int i=0;i<n;i++){
            sum ^= nums[i];
        }
        return sum;
    }
}

Unique Paths using tabulation
-----------------------------
Time Complexity - O(m*n)
Space Complexity - O(m*n)
-----------------------------
class Solution {
    public int uniquePaths(int m, int n) {
        int[][] dp = new int[m][n];
  // Enter 1 as number of ways for edges
        for(int i=0;i<m;i++){
            dp[i][0] = 1;
        }

        for(int j=0;j<n;j++){
            dp[0][j] = 1;
        }

        for(int i=1;i<m;i++){
            for(int j=1;j<n;j++){
            // Recurrence relation  - number of ways at current element is sum of number of ways at left + top
                dp[i][j] = dp[i-1][j] + dp[i][j-1];
            }
        }
        return dp[m-1][n-1];
    }
}
